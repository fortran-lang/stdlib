        ${t1}$ :: p(n) = [1_${k1}$, 2_${k1}$, 3_${k1}$, 2_${k1}$]
        ${t2}$ :: x(n) = [0.5_${k2}$, 3.5_${k2}$, -5.0_${k2}$, -10.0_${k2}$]

        ${t2}$, parameter :: ans(n) = [0.60653065971263342_${k2}$,             &
                                       0.13588822540043325_${k2}$,             &
                                       2.5230237047438022E3_${k2}$,            &
