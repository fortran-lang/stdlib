                 else
                    ! side = 'r', n is odd, and transr = 't'
                    if( lower ) then
                       ! side  ='r', n is odd, transr = 't', and uplo = 'l'
                       if( notrans ) then
                          ! side  ='r', n is odd, transr = 't', uplo = 'l', and
