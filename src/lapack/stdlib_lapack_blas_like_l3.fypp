                          else
                             call stdlib${ii}$_${ci}$trsm( 'L', 'U', 'N', diag, m2, n, alpha,a( m ), m, b( &
                                       m1, 0_${ik}$ ), ldb )
                             call stdlib${ii}$_${ci}$gemm( 'C', 'N', m1, n, m2, -cone, a( m1 ),m, b( m1, 0_${ik}$ ),&
                                        ldb, alpha, b, ldb )
                             call stdlib${ii}$_${ci}$trsm( 'L', 'L', 'C', diag, m1, n, cone,a( 0_${ik}$ ), m, b, &
                                       ldb )
                          end if
