                                    )
                          call stdlib${ii}$_${ci}$gemm( 'C', 'N', m2, n, m1, -cone, a( 0_${ik}$ ), m,b, ldb, alpha, &
                                    b( m1, 0_${ik}$ ), ldb )
                          call stdlib${ii}$_${ci}$trsm( 'L', 'U', 'C', diag, m2, n, cone,a( m1 ), m, b( m1, &
                                    0_${ik}$ ), ldb )
                       else
                          ! side  ='l', n is odd, transr = 'n', uplo = 'u', and
