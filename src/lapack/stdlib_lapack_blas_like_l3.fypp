                                    , ldb )
                          call stdlib${ii}$_dgemm( 'N', 'N', k, n, k, -one,a( k*( k+1 ) ), k, b( k, 0_${ik}$ ),&
                                     ldb,alpha, b, ldb )
                          call stdlib${ii}$_dtrsm( 'L', 'U', 'N', diag, k, n, one,a( k ), k, b, ldb )
                                    
                       end if
