                    if( notrans ) then
                       ! n is even, transr = 't', uplo = 'u', and trans = 'n'
                       call stdlib${ii}$_dsyrk( 'U', 'N', nk, k, alpha, a( 1_${ik}$, 1_${ik}$ ), lda,beta, c( nk*( nk+&
