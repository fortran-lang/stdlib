                       else
                          ! side  ='r', n is odd, transr = 'c', uplo = 'u', and
                          ! trans = 'c'
                          call stdlib${ii}$_${ci}$trsm( 'R', 'L', 'N', diag, m, n2, alpha,a( n1*n2 ), n2, b( &
                                    0_${ik}$, n1 ), ldb )
                          call stdlib${ii}$_${ci}$gemm( 'N', 'N', m, n1, n2, -cone, b( 0_${ik}$, n1 ),ldb, a( 0_${ik}$ ), &
