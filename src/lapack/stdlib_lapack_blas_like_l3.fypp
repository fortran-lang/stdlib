                    ! side = 'l', n is even, and transr = 't'
                    if( lower ) then
                       ! side  ='l', n is even, transr = 't', and uplo = 'l'
                       if( notrans ) then
                          ! side  ='l', n is even, transr = 't', uplo = 'l',
                          ! and trans = 'n'
                          call stdlib${ii}$_dtrsm( 'L', 'U', 'T', diag, k, n, alpha,a( k ), k, b, ldb )
                                    
