                       else
                          ! side  ='l', n is odd, transr = 'c', uplo = 'u', and
                          ! trans = 'c'
                          call stdlib${ii}$_${ci}$trsm( 'L', 'L', 'C', diag, m2, n, alpha,a( m1*m2 ), m2, b( &
