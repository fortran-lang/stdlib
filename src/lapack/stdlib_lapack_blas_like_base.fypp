                 ! distributed on the unit circle
                 do i = 1, il
                    x( iv+i-1 ) = exp( cmplx( zero, twopi*u( 2_${ik}$*i ),KIND=${ck}$) )
                 end do
              end if
              60 continue
           return
     end subroutine stdlib${ii}$_${ci}$larnv

