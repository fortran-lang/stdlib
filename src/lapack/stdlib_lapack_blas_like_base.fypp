                    do j = 0, k - 2
                       do i = 0, j
                          a( j, i ) = conjg( arf( ij ) )
                          ij = ij + 1_${ik}$
                       end do
                       do i = k + 1 + j, n - 1
                          a( i, k+1+j ) = arf( ij )
                          ij = ij + 1_${ik}$
                       end do
                    end do
                    do j = k - 1, n - 1
                       do i = 0, k - 1
